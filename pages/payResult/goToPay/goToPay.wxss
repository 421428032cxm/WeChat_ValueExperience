page{
  background-color: #fff
}
.system-tip{
  padding: 40rpx 0;
  color: #fd4a8b;
  line-height: 80rpx;
  text-align: center;
  border-bottom: 20rpx solid #f5f5f5;
}
.details{
  padding: 25rpx 20rpx;
  line-height: 60rpx;
}
.order-count{
  display: flex;
  justify-content: space-between;
}
.pay button{
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #fd4a8b;
    color: #fff;
    border-radius: 0;
    position: fixed;
    left: 0;
    bottom: 0;
}