/**app.wxss**/
@import "/wxParse/wxParse.wxss";
page{
  background-color: #f5f5f5;
  font-size: 30rpx;
}

.bgColor{
  background-color: #fff;
}
.line1-overflow{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.line2-overflow{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.color-fd4a8b{
  color: #fd4a8b;
}
.color-999{
  color: #999;
}
.color-666{
  color: #666666;
}
.color-ff0a0a{
  color: #ff0a0a;
}
.price-now{
  font-size: 32rpx;
  color: #fd4a8b;
  margin-right: 5rpx;
}
.price-original{
  font-size: 23rpx;
  color: #999999;
  text-decoration: line-through;
}
.space-between{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.normal-flex{
  display: flex;
  align-items: center;
}
.border-bottom-bg{
  border-bottom: 20rpx solid #f5f5f5;
}
input{
  height: 100%;
}
.bottom20{
  margin-bottom: 20rpx;
}
button{
  padding: 0;
  border: none;
}
.kefu{
  background-color: #ffffff;
  width: 100rpx;
  height: 100rpx; 
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  right: 20rpx;
  bottom: 170rpx;
   display: none;
}
.kefu image{
  display: block;
  width: 60rpx;
  height: 60rpx;
}


